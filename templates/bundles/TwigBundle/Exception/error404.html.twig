
{% set permiso = 'layouts/base.html.twig' %}
{% if app.user %}
	{% if is_granted('ROLE_ADMIN') %}
		{% set permiso = 'layouts/admin.html.twig' %}
	{% elseif is_granted('ROLE_USER') %}
		{% set permiso = 'layouts/user.html.twig' %}
	{% endif %}
{% endif %}
{% extends permiso %}

{% block title %}ERROR{% endblock %}

{% block body %}
<div  class="container-fluid" id="contentBody">
  <div class="card mb-8">
    <div class="card-header">
      ERROR
    </div>
    <div class="card-body">
          <h1>Error 404 referencia no encontrada<h1>
    </div>
  </div>
</div>
{% endblock %}